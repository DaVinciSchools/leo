{
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.updateBuildConfiguration": "automatic",
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[javascriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "eslint.format.enable": true,
    "eslint.ignoreUntitled": true,
    "eslint.options": {
        "overrideConfigFile": "server/src/main/clients/web-browser/.eslintrc.json",
        "ignorePath": "server/src/main/clients/web-browser/.eslintignore"
    },
    "java.sources.organizeImports.starThreshold": 999999,
    "java.sources.organizeImports.staticStarThreshold": 999999,
    "maven.pomfile.autoUpdateEffectivePOM": true,
    "files.eol": "\n",
}