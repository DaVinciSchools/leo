{
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.updateBuildConfiguration": "automatic",
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[javascriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[jsonc]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "eslint.format.enable": true,
    "eslint.ignoreUntitled": true,
    "eslint.options": {
        "overrideConfigFile": "server/src/main/clients/web-browser/.eslintrc.json",
        "ignorePath": "server/src/main/clients/web-browser/.eslintignore"
    },
    "java.sources.organizeImports.starThreshold": 999999,
    "java.sources.organizeImports.staticStarThreshold": 999999,
    "maven.pomfile.autoUpdateEffectivePOM": true,
    "files.eol": "\n",
    "java.format.enabled": false,
    "java.maven.downloadSources": true,
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "google-java-format.executable": "${env.HOME}/.m2/repository/com/google/googlejavaformat/google-java-format/1.18.1/google-java-format-1.18.1.jar",
    "eslint.codeActionsOnSave.rules": [
        "eslint:fix"
    ],
    "eslint.run": "onSave",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
}